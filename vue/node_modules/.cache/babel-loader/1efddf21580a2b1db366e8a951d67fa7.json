{"ast":null,"code":"export default {\n  name: \"Menu\",\n\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 10,\n      name: \"\",\n      description: \"\",\n      records: {},\n      form: {},\n      multipleSelection: [],\n      dialogFormVisible: false,\n      headerBg: 'headerBg',\n      options: []\n    };\n  },\n\n  created() {\n    // 请求分页查询数据\n    this.load();\n  },\n\n  methods: {\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n    },\n\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id); //[{},{},{}]==>[1,2,3]\n\n      this.request.post(\"/menu/del/batch\", ids).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"批量删除成功！\");\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败！\");\n        }\n      });\n    },\n\n    del(id) {\n      this.request.delete(\"/menu/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"删除成功！\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败！\");\n        }\n      });\n    },\n\n    handleEdit(row) {\n      this.form = row;\n      this.dialogFormVisible = true; //请求图标的数据\n\n      this.request.get(\"/menu/icons\").then(res => {\n        this.options = res.data;\n      });\n    },\n\n    handleAdd2(pid) {\n      this.dialogFormVisible = true;\n      this.form = {};\n\n      if (pid) {\n        this.form.pid = pid;\n      }\n    },\n\n    handleAdd1() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      this.request.post(\"/menu\", this.form).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"保存成功！\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败！\");\n        }\n      });\n    },\n\n    load() {\n      this.request.get(\"/menu\", {\n        params: {\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data;\n      });\n    },\n\n    reset() {\n      this.name = \"\";\n      this.load();\n    },\n\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAoGA;EACAA,YADA;;EAEAC;IACA;MACAC,aADA;MAEAC,QAFA;MAGAC,UAHA;MAIAC,YAJA;MAKAL,QALA;MAMAM,eANA;MAOAC,WAPA;MAQAC,QARA;MASAC,qBATA;MAUAC,wBAVA;MAWAC,oBAXA;MAYAC;IAZA;EAcA,CAjBA;;EAkBAC;IACA;IACA;EACA,CArBA;;EAsBAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACA,gDADA,CACA;;MACA;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CAdA;;IAeAC;MACA;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CAxBA;;IAyBAC;MACA;MACA,8BAFA,CAGA;;MACA;QACA;MACA,CAFA;IAGA,CAhCA;;IAiCAC;MACA;MACA;;MACA;QACA;MACA;IACA,CAvCA;;IAwCAC;MACA;MACA;IACA,CA3CA;;IA4CAC;MACA;QACA;UACA;UACA;UACA;QACA,CAJA,MAIA;UACA;QACA;MACA,CARA;IASA,CAtDA;;IAuDAC;MACA;QACAC;UACAvB;QADA;MADA,GAKAwB,IALA,CAKAC;QACA;MACA,CAPA;IAQA,CAhEA;;IAiEAC;MACA;MACA;IACA,CApEA;;IAqEAC;MACA;MACA;IACA,CAxEA;;IAyEAC;MACA;MACA;IACA;;EA5EA;AAtBA","names":["name","data","tableData","total","pageNum","pageSize","description","records","form","multipleSelection","dialogFormVisible","headerBg","options","created","methods","handleSelectionChange","delBatch","del","handleEdit","handleAdd2","handleAdd1","save","load","params","then","res","reset","handleSizeChange","handleCurrentChange"],"sourceRoot":"src/views","sources":["Menu.vue"],"sourcesContent":["<template>\n  <div>\n    <div style=\"margin: 10px 0\">\n      <el-input style=\"width: 260px\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\" v-model=\"name\"></el-input>\n      <!--<el-input class=\"ml-5\" style=\"width: 260px\" placeholder=\"请输入邮箱\" suffix-icon=\"el-icon-message\" v-model=\"email\"></el-input>-->\n      <!--<el-input style=\"width: 260px\" placeholder=\"请输入地址\" suffix-icon=\"el-icon-position\" v-model=\"address\"></el-input>-->\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\">重置</el-button>\n    </div>\n    <div style=\"margin: 10px 0\">\n      <el-button type=\"primary\" @click=\"handleAdd1\">新增<i class=\"el-icon-circle-plus-outline\"></i></el-button>\n      <el-popconfirm\n          class=\"ml-5\"\n          confirm-button-text='确定'\n          cancel-button-text='我再想想'\n          icon=\"el-icon-info\"\n          icon-color=\"red\"\n          title=\"您确定批量删除所选项吗？\"\n          @confirm=\"delBatch\">\n        <el-button type=\"danger\" class=\"ml-5\" slot=\"reference\">批量删除<i class=\"el-icon-remove-outline\"></i></el-button>\n      </el-popconfirm>\n      <!--<el-upload action=\"http://localhost:8081/user/import\" :show-file-list=\"false\" accept=\"xlsx\"-->\n      <!--           :on-success=\"handleExcelImportSuccess\" style=\"display: inline-block\">-->\n      <!--  <el-button type=\"primary\">导入<i class=\"el-icon-bottom\"></i></el-button>-->\n      <!--</el-upload>-->\n      <!--  <el-button type=\"primary\" style=\"margin-left: 10px\" @click=\"exp\">导出<i class=\"el-icon-top\"></i></el-button>-->\n    </div>\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\"\n              row-key=\"id\" default-expand-all @selection-change=\"handleSelectionChange\">\n      <el-table-column type=\"selection\"></el-table-column>\n      <el-table-column prop=\"id\" label=\"ID\" width=\"80\"></el-table-column>\n      <el-table-column prop=\"name\" label=\"名称\"></el-table-column>\n      <el-table-column prop=\"path\" label=\"路径\"></el-table-column>\n      <el-table-column prop=\"pagePath\" label=\"页面路径\"></el-table-column>\n      <el-table-column label=\"图标\" class-name=\"fontSize18\" align=\"center\" label-class-name=\"fontSize12\">\n        <template slot-scope=\"scope\">\n          <i :class=\"scope.row.icon\"/>\n        </template>\n      </el-table-column>\n      <el-table-column prop=\"description\" label=\"描述\"></el-table-column>\n      <el-table-column label=\"操作\" width=\"300\" align=\"center\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"primary\" @click=\"handleAdd2(scope.row.id)\" v-if=\"!scope.row.pid && !scope.row.path\">添加子菜单 <i\n              class=\"el-icon-plus\"></i></el-button>\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\">编辑 <i class=\"el-icon-edit\"></i></el-button>\n          <el-popconfirm\n              class=\"ml-5\"\n              confirm-button-text='确定'\n              cancel-button-text='我再想想'\n              icon=\"el-icon-info\"\n              icon-color=\"red\"\n              title=\"您确定删除吗？\"\n              @confirm=\"del(scope.row.id)\">\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-warning-outline\"></i></el-button>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div class=\"block\" style=\"padding: 10px 0;text-align: center\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pageNum\"\n          :page-sizes=\"[5, 10, 20, 30]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n    <el-dialog title=\"菜单信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\n      <el-form label-width=\"80px\" size=\"small\">\n        <el-form-item label=\"名称\">\n          <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"路径\">\n          <el-input v-model=\"form.path\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"页面路径\">\n          <el-input v-model=\"form.pagePath\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"图标\">\n          <el-select clearable v-model=\"form.icon\" placeholder=\"请选择图标\" style=\"width: 100%\">\n            <el-option v-for=\"item in options\" :key=\"item.name\" :label=\"item.name\" :value=\"item.value\">\n              <i :class=\"item.value\"/> {{ item.name }}\n            </el-option>\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"描述\">\n          <el-input v-model=\"form.description\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Menu\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 10,\n      name: \"\",\n      description: \"\",\n      records: {},\n      form: {},\n      multipleSelection: [],\n      dialogFormVisible: false,\n      headerBg: 'headerBg',\n      options: []\n    }\n  },\n  created() {\n    // 请求分页查询数据\n    this.load()\n  },\n  methods: {\n    handleSelectionChange(val) {\n      this.multipleSelection = val\n    },\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id)  //[{},{},{}]==>[1,2,3]\n      this.request.post(\"/menu/del/batch\", ids).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"批量删除成功！\")\n          this.load()\n        } else {\n          this.$message.error(\"批量删除失败！\")\n        }\n      })\n    },\n    del(id) {\n      this.request.delete(\"/menu/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"删除成功！\")\n          this.load()\n        } else {\n          this.$message.error(\"删除失败！\")\n        }\n      })\n    },\n    handleEdit(row) {\n      this.form = row\n      this.dialogFormVisible = true\n      //请求图标的数据\n      this.request.get(\"/menu/icons\").then(res => {\n        this.options = res.data\n      })\n    },\n    handleAdd2(pid) {\n      this.dialogFormVisible = true\n      this.form = {}\n      if (pid) {\n        this.form.pid = pid\n      }\n    },\n    handleAdd1() {\n      this.dialogFormVisible = true\n      this.form = {}\n    },\n    save() {\n      this.request.post(\"/menu\", this.form).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"保存成功！\")\n          this.dialogFormVisible = false\n          this.load()\n        } else {\n          this.$message.error(\"保存失败！\")\n        }\n      })\n    },\n    load() {\n      this.request.get(\"/menu\", {\n        params: {\n          name: this.name,\n        }\n      })\n          .then(res => {\n            this.tableData = res.data\n          })\n    },\n    reset() {\n      this.name = \"\"\n      this.load()\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum\n      this.load()\n    },\n\n  }\n}\n</script>\n\n<style>\n.headerBg {\n  background: rgb(236, 238, 239) !important;\n}\n\n.fontSize18 {\n  font-size: 18px;\n}\n\n.fontSize12 {\n  font-size: 12px;\n}\n</style>"]},"metadata":{},"sourceType":"module"}