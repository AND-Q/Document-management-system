{"ast":null,"code":"import { serverIp } from \"../../public/config\";\nexport default {\n  name: \"User\",\n\n  data() {\n    return {\n      serverIp: serverIp,\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 10,\n      username: \"\",\n      email: \"\",\n      address: \"\",\n      form: {},\n      multipleSelection: [],\n      dialogFormVisible: false,\n      headerBg: 'headerBg',\n      roles: []\n    };\n  },\n\n  created() {\n    // 请求分页查询数据\n    this.load();\n  },\n\n  methods: {\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n    },\n\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id); //[{},{},{}]==>[1,2,3]\n\n      this.request.post(\"/user/del/batch\", ids).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"批量删除成功！\");\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败！\");\n        }\n      });\n    },\n\n    del(id) {\n      this.request.delete(\"/user/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"删除成功！\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败！\");\n        }\n      });\n    },\n\n    handleEdit(row) {\n      this.form = row;\n      this.dialogFormVisible = true;\n    },\n\n    handleAdd() {\n      this.dialogFormVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      this.request.post(\"/user\", this.form).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"保存成功！\");\n          this.dialogFormVisible = false;\n          this.load();\n        } else {\n          this.$message.error(\"保存失败！\");\n        }\n      });\n    },\n\n    load() {\n      this.request.get(\"/user/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          username: this.username,\n          email: this.email,\n          address: this.address\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n      this.request.get(\"/role\").then(res => {\n        this.roles = res.data;\n      });\n    },\n\n    reset() {\n      this.username = \"\";\n      this.email = \"\";\n      this.address = \"\";\n      this.load();\n    },\n\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n\n    exp() {\n      window.open(`http://${serverIp}:8082/user/export`);\n    },\n\n    handleExcelImportSuccess() {\n      this.$message.success(\"文件导入成功！\");\n      this.load();\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmGA;AAEA;EACAA,YADA;;EAEAC;IACA;MACAC,kBADA;MAEAC,aAFA;MAGAC,QAHA;MAIAC,UAJA;MAKAC,YALA;MAMAC,YANA;MAOAC,SAPA;MAQAC,WARA;MASAC,QATA;MAUAC,qBAVA;MAWAC,wBAXA;MAYAC,oBAZA;MAaAC;IAbA;EAeA,CAlBA;;EAmBAC;IACA;IACA;EACA,CAtBA;;EAuBAC;IACAC;MACA;IACA,CAHA;;IAIAC;MACA,gDADA,CACA;;MACA;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CAdA;;IAeAC;MACA;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CAxBA;;IAyBAC;MACA;MACA;IACA,CA5BA;;IA6BAC;MACA;MACA;IACA,CAhCA;;IAiCAC;MACA;QACA;UACA;UACA;UACA;QACA,CAJA,MAIA;UACA;QACA;MACA,CARA;IASA,CA3CA;;IA4CAC;MACA;QACAC;UACAnB,qBADA;UAEAC,uBAFA;UAGAC,uBAHA;UAIAC,iBAJA;UAKAC;QALA;MADA,GASAgB,IATA,CASAC;QACA;QACA;MACA,CAZA;MAaA;QACA;MACA,CAFA;IAGA,CA7DA;;IA8DAC;MACA;MACA;MACA;MACA;IACA,CAnEA;;IAoEAC;MACA;MACA;IACA,CAvEA;;IAwEAC;MACA;MACA;IACA,CA3EA;;IA4EAC;MACAC;IACA,CA9EA;;IA+EAC;MACA;MACA;IACA;;EAlFA;AAvBA","names":["name","data","serverIp","tableData","total","pageNum","pageSize","username","email","address","form","multipleSelection","dialogFormVisible","headerBg","roles","created","methods","handleSelectionChange","delBatch","del","handleEdit","handleAdd","save","load","params","then","res","reset","handleSizeChange","handleCurrentChange","exp","window","handleExcelImportSuccess"],"sourceRoot":"src/views","sources":["User.vue"],"sourcesContent":["<template>\n  <div>\n    <div style=\"margin: 10px 0\">\n      <el-input style=\"width: 260px\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\"\n                v-model=\"username\"></el-input>\n      <el-input class=\"ml-5\" style=\"width: 260px\" placeholder=\"请输入邮箱\" suffix-icon=\"el-icon-message\"\n                v-model=\"email\"></el-input>\n      <el-input style=\"width: 260px\" placeholder=\"请输入地址\" suffix-icon=\"el-icon-position\"\n                v-model=\"address\"></el-input>\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\">搜索</el-button>\n      <el-button style=\"margin-left: 5px\" type=\"warning\" @click=\"reset\">重置</el-button>\n    </div>\n    <div style=\"margin: 10px 0\">\n      <el-button type=\"primary\" @click=\"handleAdd\">新增<i class=\"el-icon-circle-plus-outline\"></i></el-button>\n      <el-popconfirm\n          class=\"ml-5\"\n          confirm-button-text='确定'\n          cancel-button-text='我再想想'\n          icon=\"el-icon-info\"\n          icon-color=\"red\"\n          title=\"您确定批量删除所选项吗？\"\n          @confirm=\"delBatch\">\n        <el-button type=\"danger\" class=\"ml-5\" slot=\"reference\">批量删除<i class=\"el-icon-remove-outline\"></i></el-button>\n      </el-popconfirm>\n      <el-upload :action=\"'http://'+ serverIp +':8082/user/import'\" :show-file-list=\"false\" accept=\"xlsx\"\n                 :on-success=\"handleExcelImportSuccess\" style=\"display: inline-block\">\n        <el-button type=\"primary\">导入<i class=\"el-icon-bottom\"></i></el-button>\n      </el-upload>\n        <el-button type=\"primary\" style=\"margin-left: 10px\" @click=\"exp\">导出<i class=\"el-icon-top\"></i></el-button>\n    </div>\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"headerBg\"\n              @selection-change=\"handleSelectionChange\">\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\n      <el-table-column prop=\"id\" label=\"ID\" width=\"80\"></el-table-column>\n      <el-table-column prop=\"username\" label=\"用户名\" width=\"140\"></el-table-column>\n      <el-table-column prop=\"role\" label=\"角色\"></el-table-column>\n      <el-table-column prop=\"nickname\" label=\"昵称\" width=\"120\"></el-table-column>\n      <el-table-column prop=\"email\" label=\"邮箱\"></el-table-column>\n      <el-table-column prop=\"phone\" label=\"电话\"></el-table-column>\n      <el-table-column prop=\"address\" label=\"地址\"></el-table-column>\n      <el-table-column label=\"操作\" width=\"200\" align=\"center\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\">编辑 <i class=\"el-icon-edit\"></i></el-button>\n          <el-popconfirm\n              class=\"ml-5\"\n              confirm-button-text='确定'\n              cancel-button-text='我再想想'\n              icon=\"el-icon-info\"\n              icon-color=\"red\"\n              title=\"您确定删除吗？\"\n              @confirm=\"del(scope.row.id)\">\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-warning-outline\"></i></el-button>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div class=\"block\" style=\"padding: 10px 0;text-align: center\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pageNum\"\n          :page-sizes=\"[5, 10, 20, 30]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n    <el-dialog title=\"用户信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\">\n      <el-form label-width=\"80px\" size=\"small\">\n        <el-form-item label=\"用户名\">\n          <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"角色\">\n          <el-select clearable v-model=\"form.role\" placeholder=\"请选择角色\" style=\"width: 100%\">\n            <el-option v-for=\"item in roles\" :key=\"item.name\" :label=\"item.name\" :value=\"item.flag\" />\n          </el-select>\n        </el-form-item>\n        <el-form-item label=\"昵称\">\n          <el-input v-model=\"form.nickname\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"邮箱\">\n          <el-input v-model=\"form.email\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"电话\">\n          <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"地址\">\n          <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport {serverIp} from \"../../public/config\";\n\nexport default {\n  name: \"User\",\n  data() {\n    return {\n      serverIp:serverIp,\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 10,\n      username: \"\",\n      email: \"\",\n      address: \"\",\n      form: {},\n      multipleSelection: [],\n      dialogFormVisible: false,\n      headerBg: 'headerBg',\n      roles:[]\n    }\n  },\n  created() {\n    // 请求分页查询数据\n    this.load()\n  },\n  methods: {\n    handleSelectionChange(val) {\n      this.multipleSelection = val\n    },\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id)  //[{},{},{}]==>[1,2,3]\n      this.request.post(\"/user/del/batch\", ids).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"批量删除成功！\")\n          this.load()\n        } else {\n          this.$message.error(\"批量删除失败！\")\n        }\n      })\n    },\n    del(id) {\n      this.request.delete(\"/user/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"删除成功！\")\n          this.load()\n        } else {\n          this.$message.error(\"删除失败！\")\n        }\n      })\n    },\n    handleEdit(row) {\n      this.form = row\n      this.dialogFormVisible = true\n    },\n    handleAdd() {\n      this.dialogFormVisible = true\n      this.form = {}\n    },\n    save() {\n      this.request.post(\"/user\", this.form).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"保存成功！\")\n          this.dialogFormVisible = false\n          this.load()\n        } else {\n          this.$message.error(\"保存失败！\")\n        }\n      })\n    },\n    load() {\n      this.request.get(\"/user/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          username: this.username,\n          email: this.email,\n          address: this.address\n        }\n      })\n          .then(res => {\n            this.tableData = res.data.records\n            this.total = res.data.total\n          })\n      this.request.get(\"/role\").then(res =>{\n        this.roles = res.data\n      })\n    },\n    reset() {\n      this.username = \"\"\n      this.email = \"\"\n      this.address = \"\"\n      this.load()\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum\n      this.load()\n    },\n    exp(){\n      window.open(`http://${serverIp}:8082/user/export`)\n    },\n    handleExcelImportSuccess(){\n      this.$message.success(\"文件导入成功！\")\n      this.load()\n    }\n\n  }\n}\n</script>\n\n<style>\n.headerBg {\n  background: rgb(236, 238, 239) !important;\n}\n</style>"]},"metadata":{},"sourceType":"module"}