{"ast":null,"code":"import \"core-js/modules/web.dom-exception.stack.js\";\nimport { serverIp } from \"../../public/config\";\nexport default {\n  name: \"File\",\n\n  data() {\n    return {\n      serverIp: serverIp,\n      tableData: [],\n      name: '',\n      multipleSelection: [],\n      pageNum: 1,\n      pageSize: 10,\n      total: 0\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      this.request.get(\"/file/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    changeEnable(row) {\n      this.request.post(\"/file/update\", row).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"操作成功\");\n        }\n      });\n    },\n\n    del(id) {\n      this.request.delete(\"/file/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n\n    handleSelectionChange(val) {\n      console.log(val);\n      this.multipleSelection = val;\n    },\n\n    delBatch() {\n      let ids = this.multipleSelection.map(v => v.id); // [{}, {}, {}] => [1,2,3]\n\n      this.request.post(\"/file/del/batch\", ids).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"批量删除成功\");\n          this.load();\n        } else {\n          this.$message.error(\"批量删除失败\");\n        }\n      });\n    },\n\n    reset() {\n      this.name = \"\";\n      this.load();\n    },\n\n    handleSizeChange(pageSize) {\n      console.log(pageSize);\n      this.pageSize = pageSize;\n      this.load();\n    },\n\n    handleCurrentChange(pageNum) {\n      console.log(pageNum);\n      this.pageNum = pageNum;\n      this.load();\n    },\n\n    handleFileUploadSuccess() {\n      // console.log(res)\n      this.$message.success(\"上传成功!\");\n      this.load();\n    },\n\n    download(url) {\n      window.open(url);\n    },\n\n    preview(url) {\n      window.open('https://file.keking.cn/onlinePreview?url=' + encodeURIComponent(window.btoa(url)));\n    }\n\n  }\n};","map":{"version":3,"mappings":";AAiFA;AAEA;EACAA,YADA;;EAEAC;IACA;MACAC,kBADA;MAEAC,aAFA;MAGAH,QAHA;MAIAI,qBAJA;MAKAC,UALA;MAMAC,YANA;MAOAC;IAPA;EASA,CAZA;;EAaAC;IACA;EACA,CAfA;;EAgBAC;IACAC;MACA;QACAC;UACAN,qBADA;UAEAC,uBAFA;UAGAN;QAHA;MADA,GAMAY,IANA,CAMAC;QAEA;QACA;MAEA,CAXA;IAYA,CAdA;;IAeAC;MACA;QACA;UACA;QACA;MACA,CAJA;IAKA,CArBA;;IAsBAC;MACA;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CA/BA;;IAgCAC;MACAC;MACA;IACA,CAnCA;;IAoCAC;MACA,gDADA,CACA;;MACA;QACA;UACA;UACA;QACA,CAHA,MAGA;UACA;QACA;MACA,CAPA;IAQA,CA9CA;;IA+CAC;MACA;MACA;IACA,CAlDA;;IAmDAC;MACAH;MACA;MACA;IACA,CAvDA;;IAwDAI;MACAJ;MACA;MACA;IACA,CA5DA;;IA6DAK;MACA;MACA;MACA;IACA,CAjEA;;IAkEAC;MACAC;IACA,CApEA;;IAqEAC;MACAD;IACA;;EAvEA;AAhBA","names":["name","data","serverIp","tableData","multipleSelection","pageNum","pageSize","total","created","methods","load","params","then","res","changeEnable","del","handleSelectionChange","console","delBatch","reset","handleSizeChange","handleCurrentChange","handleFileUploadSuccess","download","window","preview"],"sourceRoot":"src/views","sources":["File.vue"],"sourcesContent":["<template>\n  <div>\n    <div style=\"margin: 10px 0\">\n      <el-input class=\"ml-5\" style=\"width: 200px\" placeholder=\"请输入名称\" suffix-icon=\"el-icon-search\" v-model=\"name\"></el-input>\n      <el-button type=\"primary\" @click=\"load\">搜索</el-button>\n      <el-button type=\"warning\" @click=\"reset\">重置</el-button>\n    </div>\n    <div style=\"margin: 10px 0\">\n      <el-upload :action=\"'http://'+ serverIp +':8082/file/upload'\" :show-file-list=\"false\"\n                 :on-success=\"handleFileUploadSuccess\" style=\"display: inline-block\">\n        <el-button type=\"primary\" class=\"ml-5\">上传文件 <i class=\"el-icon-top\"></i></el-button>\n      </el-upload>\n      <el-popconfirm\n          class=\"ml-5\"\n          confirm-button-text='确定'\n          cancel-button-text='我再想想'\n          icon=\"el-icon-info\"\n          icon-color=\"red\"\n          title=\"您确定批量删除这些数据吗？\"\n          @confirm=\"delBatch\"\n      >\n        <el-button type=\"danger\" slot=\"reference\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\n      </el-popconfirm>\n\n    </div>\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"'headerBg'\"\n              @selection-change=\"handleSelectionChange\">\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\n      <el-table-column prop=\"id\" label=\"ID\" width=\"80\"></el-table-column>\n      <el-table-column prop=\"name\" label=\"文件名称\"></el-table-column>\n      <el-table-column prop=\"type\" label=\"文件类型\"></el-table-column>\n      <el-table-column prop=\"size\" label=\"文件大小(kb)\"></el-table-column>\n      <el-table-column label=\"预览\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"primary\" @click=\"preview(scope.row.url)\">预览</el-button>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"下载\">\n        <template slot-scope=\"scope\">\n          <el-button type=\"primary\" @click=\"download(scope.row.url)\">下载</el-button>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"启用\">\n        <template slot-scope=\"scope\">\n          <el-switch v-model=\"scope.row.enable\" active-color=\"#13ce66\" inactive-color=\"#ccc\"\n                     @change=\"changeEnable(scope.row)\"></el-switch>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" width=\"200\" align=\"center\">\n        <template slot-scope=\"scope\">\n          <el-popconfirm\n              class=\"ml-5\"\n              confirm-button-text='确定'\n              cancel-button-text='我再想想'\n              icon=\"el-icon-info\"\n              icon-color=\"red\"\n              title=\"您确定删除吗？\"\n              @confirm=\"del(scope.row.id)\"\n          >\n            <el-button type=\"danger\" slot=\"reference\">删除 <i class=\"el-icon-remove-outline\"></i></el-button>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <div style=\"padding: 10px 0;text-align: center \">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pageNum\"\n          :page-sizes=\"[2, 5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n\n  </div>\n</template>\n\n<script>\nimport {serverIp} from \"../../public/config\";\n\nexport default {\n  name: \"File\",\n    data() {\n      return {\n        serverIp:serverIp,\n        tableData: [],\n        name: '',\n        multipleSelection: [],\n        pageNum: 1,\n        pageSize: 10,\n        total: 0\n      }\n    },\n    created() {\n      this.load()\n    },\n    methods: {\n      load() {\n        this.request.get(\"/file/page\", {\n          params: {\n            pageNum: this.pageNum,\n            pageSize: this.pageSize,\n            name: this.name,\n          }\n        }).then(res => {\n\n          this.tableData = res.data.records\n          this.total = res.data.total\n\n        })\n      },\n      changeEnable(row) {\n        this.request.post(\"/file/update\", row).then(res => {\n          if (res.code === '200') {\n            this.$message.success(\"操作成功\")\n          }\n        })\n      },\n      del(id) {\n        this.request.delete(\"/file/\" + id).then(res => {\n          if (res.code === '200') {\n            this.$message.success(\"删除成功\")\n            this.load()\n          } else {\n            this.$message.error(\"删除失败\")\n          }\n        })\n      },\n      handleSelectionChange(val) {\n        console.log(val)\n        this.multipleSelection = val\n      },\n      delBatch() {\n        let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\n        this.request.post(\"/file/del/batch\", ids).then(res => {\n          if (res.code === '200') {\n            this.$message.success(\"批量删除成功\")\n            this.load()\n          } else {\n            this.$message.error(\"批量删除失败\")\n          }\n        })\n      },\n      reset() {\n        this.name = \"\"\n        this.load()\n      },\n      handleSizeChange(pageSize) {\n        console.log(pageSize)\n        this.pageSize = pageSize\n        this.load()\n      },\n      handleCurrentChange(pageNum) {\n        console.log(pageNum)\n        this.pageNum = pageNum\n        this.load()\n      },\n      handleFileUploadSuccess() {\n        // console.log(res)\n        this.$message.success(\"上传成功!\")\n        this.load()\n      },\n      download(url) {\n        window.open(url)\n      },\n      preview(url) {\n        window.open('https://file.keking.cn/onlinePreview?url=' + encodeURIComponent(window.btoa((url))))\n      },\n    }\n}\n</script>\n\n<style scoped>\n\n</style>"]},"metadata":{},"sourceType":"module"}